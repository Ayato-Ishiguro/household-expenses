{
    "plugins": ["import"],
    "extends": ["eslint:recommended", "plugin:import/typescript"],
    "rules": {
        "import/order": [
            "error",
            {
                "groups": [
                    "builtin",
                    "external",
                    "internal",
                    "parent",
                    "sibling",
                    "index"
                ],
                "alphabetize": { "order": "asc", "caseInsensitive": true }
            }
        ]
    }
}
